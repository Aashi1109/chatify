{
  "name": "chatify",
  "version": "1.0.0",
  "description": "This is a basic chat app made using NextJs and ExpressJs",
  "scripts": {
    "start": "npm-run-all --parallel server client-dev",
    "server": "nodemon --exec ts-node server/index.ts",
    "client-dev": "cd client && next dev",
    "client-build": "cd client && next build",
    "client-start": "cd client && next start",
    "client-lint": "cd client && next lint"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.2.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "formik": "^2.4.5",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.5",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
