{
  "name": "dev_chatify",
  "version": "1.0.0",
  "description": "This is a basic chat app made using ReactJS and ExpressJs",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel server:dev client:dev",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:lint": "cd client && npm run lint",
    "server:seed": "cd server && npm run seed",
    "client:preview": "cd client && vite preview"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
