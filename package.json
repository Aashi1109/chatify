{
  "name": "chatify",
  "version": "1.0.0",
  "description": "This is a basic chat app made using ReactJS and ExpressJs",
  "scripts": {
    "start:dev": "npm-run-all --parallel server:dev client:dev",
    "server:dev": "nodemon --exec ts-node --project server/tsconfig.json -r tsconfig-paths/register server/index.ts",
    "client:dev": "cd client && vite",
    "client:build": "cd client && tsc && vite build",
    "client:lint": "cd client && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "server:seed": "ts-node --project server/tsconfig.json -r tsconfig-paths/register server/misc/seeder.ts",
    "client:preview": "cd client && vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.2.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "formik": "^2.4.5",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.2",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.5",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsconfig-paths": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@redocly/cli": "^1.12.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
